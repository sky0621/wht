# GraphQL schema example
#
# https://gqlgen.com/getting-started/

# Global Object Identification
interface Node {
  id: ID!
}

type Query {
  node(id: ID!): Node
}

type Mutation {
  noop(input: NoopInput): NoopPayload
}

input NoopInput {
  clientMutationId: String
}

type NoopPayload {
  clientMutationId: String
}
